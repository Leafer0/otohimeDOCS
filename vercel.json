{
  "version": 2,
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/",
      "destination": "/public/SY.html"
    },
    {
      "source": "/login",
      "destination": "/public/DL.html"
    },
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    }
  ]
}

