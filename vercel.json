{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/js/**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "public/pic/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "http://47.98.223.44:11451/$1",
      "headers": {
        "Access-Control-Allow-Origin": "https://otohime-docs.vercel.app",
        "Access-Control-Allow-Methods": "GET,POST,OPTIONS"
      }
    },
    {
      "src": "/login",
      "dest": "/public/DL.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "src": "/",
      "dest": "/public/SY.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/pic/(.*)",
      "dest": "/public/pic/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1",
      "status": 200
    }
  ]
}