{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/js/**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "public/pic/**/*",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [

    { "source": "/", "destination": "/public/SY.html" },
    { "source": "/login", "destination": "/public/DL.html" },
    

    {
      "source": "/api/(.*)",
      "destination": "http://47.98.223.44:11451/$1"
    },
    
 
    { "source": "/js/(.*)", "destination": "/public/js/$1" },
    { "source": "/pic/(.*)", "destination": "/public/pic/$1" },
    
 
    { "source": "/(.*)", "destination": "/public/$1" }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://otohime-docs.vercel.app"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,POST,PUT,DELETE,OPTIONS"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}